{
  "name": "@webpods/tinqer",
  "version": "0.0.15",
  "description": "LINQ to SQL for TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --exit",
    "test:grep": "mocha --exit --grep",
    "test:build": "tsc -p tsconfig.test.json --noEmit",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix"
  },
  "dependencies": {
    "oxc-parser": "^0.90.0"
  },
  "devDependencies": {
    "@types/chai": "5.2.2",
    "@types/estree": "^1.0.8",
    "@types/mocha": "10.0.10",
    "@types/node": "24.3.3",
    "chai": "5.3.3",
    "mocha": "11.7.2",
    "ts-node": "^10.9.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "keywords": [
    "linq",
    "sql",
    "query",
    "builder",
    "typescript",
    "orm",
    "database",
    "postgres",
    "mysql",
    "sqlite"
  ],
  "author": "WebPods",
  "license": "MIT"
}
